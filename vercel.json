{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "dist/ci-cd/vercel/init.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/ci-cd/vercel/init.js"
    }
  ]
}
